database: _dblist
privileges: 'ALL PRIVILEGES' ALTER 'ALTER ROUTINE' CREATE 'CREATE ROUTINE' 'CREATE TEMPORARY TABLES' 'CREATE VIEW' DELETE DROP EVENT EXECUTE INDEX INSERT 'LOCK TABLES' REFERENCES SELECT 'SHOW VIEW' TRIGGER UPDATE
